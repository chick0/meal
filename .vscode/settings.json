{
    "files.exclude": {
        ".git/": true,
        ".github/": true,
        ".vscode/": true,
        ".yarn/": true,
        "node_modules/": true
    },
    "[javascript]": {
        "editor.maxTokenizationLineLength": 2500,
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true,
    },
    "[svelte]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    }
}